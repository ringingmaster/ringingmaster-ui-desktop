<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
       default-autowire="byName"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:c="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd">


    <bean id="toolBar" class="com.concurrentperformance.fxutils.toolbar.InjectableToolBar">
        <property name="toolBarElements">
            <util:list>
                <bean class="com.concurrentperformance.ringingmaster.fxui.desktop.documentmanager.NewDocumentButton"/>
                <bean class="com.concurrentperformance.ringingmaster.fxui.desktop.documentmanager.OpenDocumentButton"/>
                <bean class="com.concurrentperformance.ringingmaster.fxui.desktop.documentmanager.SaveDocumentButton"/>
                <bean class="javafx.scene.control.Separator" p:orientation="VERTICAL"/>
                <bean class="com.concurrentperformance.ringingmaster.fxui.desktop.undo.UndoButton"/>
                <bean class="com.concurrentperformance.ringingmaster.fxui.desktop.undo.RedoButton"/>
                <bean class="javafx.scene.control.Separator" p:orientation="VERTICAL"/>
                <bean class="com.concurrentperformance.ringingmaster.fxui.desktop.edit.CutButton"/>
                <bean class="com.concurrentperformance.ringingmaster.fxui.desktop.edit.CopyButton"/>
                <bean class="com.concurrentperformance.ringingmaster.fxui.desktop.edit.PasteButton"/>
                <bean class="javafx.scene.control.Separator" p:orientation="VERTICAL"/>
                <bean class="com.concurrentperformance.ringingmaster.fxui.desktop.notationpanel.AddNotationButton"/>
            </util:list>
        </property>
    </bean>


</beans>
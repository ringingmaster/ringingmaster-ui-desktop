<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
       default-autowire="byName"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:c="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd">


    <bean id="event.undo" class="com.concurrentperformance.ringingmaster.ui.desktop.undo.UndoEvent"/>
    <bean id="event.redo" class="com.concurrentperformance.ringingmaster.ui.desktop.undo.RedoEvent"/>

    <bean id="event.cut" class="com.concurrentperformance.ringingmaster.ui.desktop.edit.CutEvent"/>
    <bean id="event.copy" class="com.concurrentperformance.ringingmaster.ui.desktop.edit.CopyEvent"/>
    <bean id="event.paste" class="com.concurrentperformance.ringingmaster.ui.desktop.edit.PasteEvent"/>

</beans>
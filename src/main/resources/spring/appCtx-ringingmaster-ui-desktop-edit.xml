<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       default-autowire="byName"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">


    <bean id="event.undo" class="org.ringingmaster.ui.desktop.undo.UndoEvent"/>
    <bean id="event.redo" class="org.ringingmaster.ui.desktop.undo.RedoEvent"/>

    <bean id="event.cut" class="org.ringingmaster.ui.desktop.edit.CutEvent"/>
    <bean id="event.copy" class="org.ringingmaster.ui.desktop.edit.CopyEvent"/>
    <bean id="event.paste" class="org.ringingmaster.ui.desktop.edit.PasteEvent"/>

</beans>